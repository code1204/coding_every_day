// 704. Binary Search
  public int search(int[] nums, int target) {
    if (nums==null || nums.length==0) return -1;
    int left=0, right=nums.length-1;
    // most strict condition, so I can put right= mid-1,left=mid-1
    while (left <= right){
      int mid = left + (right-left)/2;
      if (nums[mid] == target){
        return mid;
      }
      else if(nums[mid] > target){
        right = mid -1;
      }
      else{
        left = mid+1;
      }

    }
    return -1;

  }
